def eulerODE2(f, t0, y00, y10, nmax, h):
    # f: function
    # t0: starting time
    # y00: starting value of y
    # y10: starting value of y'
    # nmax: number of iterations
    # h: stepsize
    y0 = y00
    y1 = y10
    t = t0
    t_values = [0]
    y0_values = [y0]
    y1_values = [y1]
    for i in range(1, nmax+1):
        y0 = y0 + y1 * h
        y1 = y1 + f(t, y0, y1) * h
        t += h
        t_values.append(t)
        y0_values.append(y0)
        y1_values.append(y1)
    return np.array([t_values, y0_values, y1_values])








