def eulerODE(f, t0, y0, nmax, h):
    # f: function
    # t0: starting time
    # y0: starting value
    # nmax: number of iterations
    # h: stepsize
    y = y0
    t = t0
    t_values = [0]
    y_values = [y]
    for i in range(1, nmax+1):
        y = y + f(t, y) * h
        t += h
        t_values.append(t)
        y_values.append(y)
    return np.array([t_values, y_values])


