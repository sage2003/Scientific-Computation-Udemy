solution2 = integrate.solve_ivp(f_ODE, [tStart, tEnd], [1.1*x0, 0.5+y0, 3.5+z0], method = "RK45", t_eval=np.linspace(tStart, tEnd, 5001), args=(a, b, c))


plot_traj = plt.axes(projection="3d")
